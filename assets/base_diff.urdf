<?xml version="1.0" ?>
<robot name="base_diff">
  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
      <mass value="40"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
      <geometry>
        <box size="0.45 0.35 0.1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
      <geometry>
        <box size="0.45 0.35 0.1"/>
      </geometry>
    </collision>
  </link>

  <link name="wheel_left">
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.08" length="0.04"/>
      </geometry>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.08" length="0.04"/>
      </geometry>
    </collision>
  </link>

  <link name="wheel_right">
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.08" length="0.04"/>
      </geometry>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.08" length="0.04"/>
      </geometry>
    </collision>
  </link>

  <joint name="left_wheel_joint" type="revolute">
    <origin xyz="0 0.17 0.08" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="wheel_left"/>
    <axis xyz="0 1 0"/>
    <limit effort="200" velocity="50" lower="-100" upper="100"/>
  </joint>

  <joint name="right_wheel_joint" type="revolute">
    <origin xyz="0 -0.17 0.08" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="wheel_right"/>
    <axis xyz="0 1 0"/>
    <limit effort="200" velocity="50" lower="-100" upper="100"/>
  </joint>
</robot>
