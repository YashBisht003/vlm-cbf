<?xml version="1.0" ?>
<robot name="base_omni">
  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0.06" rpy="0 0 0"/>
      <mass value="60"/>
      <inertia ixx="1.2" ixy="0" ixz="0" iyy="1.2" iyz="0" izz="1.2"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.06" rpy="0 0 0"/>
      <geometry>
        <box size="0.55 0.45 0.12"/>
